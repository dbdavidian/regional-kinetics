{
    "extends": "airbnb",
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "max-len": ["error", { "code": 200 }],
        "react/jsx-one-expression-per-line": 0,
        "import/no-named-as-default": 0,
        "import/no-named-as-default-member": 0,
        "jsx-a11y/anchor-is-valid": 0
    },
    "env": {
        "browser": true,
        "jest": true
      }
}